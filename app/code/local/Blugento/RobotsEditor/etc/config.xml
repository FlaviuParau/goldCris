<config>
    <modules>
        <Blugento_RobotsEditor>
            <version>1.0.0</version>
        </Blugento_RobotsEditor>
    </modules>

    <global>
        <models>
            <blugento_robotseditor>
                <class>Blugento_RobotsEditor_Model</class>
            </blugento_robotseditor>
        </models>
        <helpers>
            <blugento_robotseditor>
                <class>Blugento_RobotsEditor_Helper</class>
            </blugento_robotseditor>
        </helpers>
    </global>

    <adminhtml>
        <translate>
            <modules>
                <blugento_robotseditor>
                    <files>
                        <default>Blugento_RobotsEditor.csv</default>
                    </files>
                </blugento_robotseditor>
            </modules>
        </translate>
    </adminhtml>

    <default>
        <blugento_robotseditor>
            <general>
                <file_content></file_content>
            </general>
        </blugento_robotseditor>
    </default>
</config>
