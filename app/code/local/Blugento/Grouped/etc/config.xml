<config>
    <modules>
        <Blugento_Grouped>
            <version>1.0.1</version>
        </Blugento_Grouped>
    </modules>

    <global>
        <helpers>
            <blugento_grouped>
                <class>Blugento_Grouped_Helper</class>
            </blugento_grouped>
        </helpers>
    </global>

    <frontend>
        <layout>
            <updates>
                <blugento_grouped>
                    <file>blugento_grouped.xml</file>
                </blugento_grouped>
            </updates>
        </layout>

        <translate>
            <modules>
                <blugento_grouped>
                    <files>
                        <default>Blugento_Grouped.csv</default>
                    </files>
                </blugento_grouped>
            </modules>
        </translate>
    </frontend>
</config>